    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.2.1",
    "i18next-resources-to-backend": "^1.2.1",
    "lucide-react": "^0.514.0",
    "meilisearch": "^0.51.0",
    "next": "15.3.4",
    "next-auth": "5.0.0-beta.25",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.5.2",
    "resend": "^4.5.2",
